{
  "extends": "./.nuxt/tsconfig.json",
  "compilerOptions": {
    "noImplicitOverride": true,
    "noPropertyAccessFromIndexSignature": true,
    "noUncheckedIndexedAccess": true,

    "baseUrl": ".",
    "paths": {
      "@/app/*": ["src/app/*"],
      "@/*": ["src/*"],
      "#imports": [".nuxt/imports.d.ts"]
    }
  }
}
